{
  "type": "project",
  "name": "leon-zadravec/test-blog-project",
  "description": "A test blog project for xentral",
  "license": "proprietary",
  "require": {
    "php": "^8.0",
    "twig/twig": "^3.0"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^9"
  }
}
